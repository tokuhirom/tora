use Test::More *;

my $a = [];
$a.push(1);
$a.push(2);
is($a.tora(), "[1,2]");

is($a.pop(), 2);
is($a.tora(), "[1]");

$a.unshift(10);
is($a.tora(), "[10,1]");

my $n = $a.shift();
is($n, 10);
is($a.tora(), "[1]");

$a.push(5);
is($a.tora(), '[1,5]');
is($a.reverse().tora(), '[5,1]');

done_testing;
