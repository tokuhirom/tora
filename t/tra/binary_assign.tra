use Test::More *;

{
    my $x = 4649;
    $x += 39;
    is($x, 4649+39);
}

{
    my $x = 4649;
    $x -= 39;
    is($x, 4649-39);
}

{
    my $x = 4649;
    $x *= 39;
    is($x, 4649*39);
}

{
    my $x = 4649;
    $x /= 39;
    is($x, 4649/39);
}

{
    my $x = 4649;
    $x |= 39;
    is($x, 4649|39);
}

{
    my $x = 4649;
    $x &= 39;
    is($x, 4649&39);
}

{
    my $x = 4649;
