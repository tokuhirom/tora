use Test::More *;

my $d = opendir('t/dat/dir');
my $a = [];
while (my $e = $d.read()) {
    $a.push($e);
}
$a = $a.sort();
is($a[0], '.');
is($a[1], '..');
is($a[2], 'x');
is($a[3], 'y');
is($a[4], 'z');

my $p = [];
for (my $e in opendir('t/dat/dir')) {
    $p.push($e);
}
is(JSON.encode($p.sort()), '[".","..","x","y","z"]');

done_testing();
